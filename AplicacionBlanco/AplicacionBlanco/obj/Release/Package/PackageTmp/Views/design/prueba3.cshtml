
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Mi aplicación ASP.NET</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")

    <script type="text/javascript">
        window.onload = function () { document.getElementByName('impacto').focus(); }
    </script>
</head>
<body>
    Luis, 16:38
    <header>
        <input id="activar" name="activar" type="checkbox">

        <div class="contenedor">


            <nav class="menu">

                <!-- Nav tabs -->
                <ul class="ml-md-3 nav nav-tabs" role="tablist" style=" margin-left:19%; display:block !important;">
                 
                    <li id="atras" class="ml-md-4 " style="padding-left:100px;">
                        <a href="javascript:history.back();"><img src="~/Content/img/atras.png" style="width: 20px">Atras</a>
                    </li>
                    <li class="ml-md-4 ">
                        <a  data-toggle="tab" href="#home" role="tab">
                            Impactos de Eta
                        </a>
                    </li>

                    <li class="ml-md-4 ">
                        <a  data-toggle="tab"
                           href="#profile" role="tab">Trayectoria de Huracanes 2020</a>
                    </li>

                    <li class="ml-md-4 ">
                        <a  data-toggle="tab"
                           href="#messages" role="tab">Fuerza Huracán ETA</a>
                    </li>

                    <li class="ml-md-4 ">
                        <a data-toggle="tab" href="#settings" role="tab">
                            Fuerza Huracán IOTA
                        </a>
                    </li>


                </ul>
            </nav>

            <label class="label" for="activar"></label>

        </div>
    </header>
    <br />
    
    <!-- Tab panes -->
    <div class="tab-content" style=" height:800px; margin-left19%;">
        <div class="tab-pane active" id="home" role="tabpanel">
            <iframe src="https://app.powerbi.com/view?r=eyJrIjoiYjgzMDc4YWQtMGI0ZC00MTFmLWJlZTgtNTFiOTM0MGViM2FlIiwidCI6IjhmYmFhNWJmLTJlY2MtNGRjOC1iNTZiLThmOTJlMzA3ZjA3NiIsImMiOjR9"></iframe>
        </div>

        <div class="tab-pane" id="profile" role="tabpanel">
            <iframe class="embed-responsive embed-responsive-16by9" id="powerbi"
                    style="margin: 0 auto; width: 90%; height: 780px; "
                    src="https://ide.dataintelligence-group.com/mapstore/#/context/huracanes/258"></iframe>
        </div>

        <div class="tab-pane" id="messages" role="tabpanel">
            <iframe class="embed-responsive embed-responsive-16by9" id="powerbi"
                    style="margin: 0 auto; width: 90%; height: 780px; "
                    src="https://ide.dataintelligence-group.com/mapstore/#/context/huracanes/259"
                    frameborder="0"></iframe>
        </div>

        <div class="tab-pane" id="settings" role="tabpanel">
            <iframe class=" embed-responsive embed-responsive-16by9 " id="powerbi"
                    style="margin: 0 auto; width: 90%; height: 780px;"
                    src="https://ide.dataintelligence-group.com/mapstore/#/context/huracanes/260"
                    frameborder="0"></iframe>
        </div>

    </div>
</body>
</html>


