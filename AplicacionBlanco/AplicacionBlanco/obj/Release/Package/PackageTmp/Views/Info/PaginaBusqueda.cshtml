    @* filtro del buscador*@
    <script>
        // Escribe en keyup evento de la palabra clave input element
        $(document).ready(function () {
            $("#buscador1").keyup(function () {
                _this = this;
                //Mustra solamente los que parecidos al TR Y ESCONDE LOS DEMAS
                $.each($("#buscador tbody tr"), function () {
                    if ($(this).text().toLowerCase().indexOf($(_this).val().toLowerCase()) === -1)
                        $(this).hide();
                    else
                        $(this).show();
                });

            });

        });
    </script>

