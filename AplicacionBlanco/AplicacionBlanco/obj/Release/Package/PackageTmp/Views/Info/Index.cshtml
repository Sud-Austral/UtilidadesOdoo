        <!--Modal 2-->
        <div class="container sticky-top" style="background:#007bff; background: linear-gradient(to right, #0062E6, #33AEFF); ">
            <div class="row">
                <div class="col-md-9 col-sm-9 text-center m-text-center">
                    <h5 class="mt-md-3 mb-md-3">
                        Acceso inmediato e ilimitado a todas las estadísticas por 49 € / mes
                    </h5>
                </div>
                <div class="col-md-3 col-sm-3  text-right mb-md-4">
                    <a href="#" target="_blank" class="btn btn-white">Precios y Acceso</a>
                </div>
            </div>
        </div>

        <section class=" row pricing py-5 mb-md-5" style="box-sizing: border-box; width: 100%;">
            <div class="container no-gutters ml-md-5 col-md-12">
                <div class="row display-block">
                    <!-- Free -->
                    <div class="col-lg-3 mt-md-4">
                        <div class="card mb-5 ">
                            <div class="card-body">
                                <h4 class=" text-center">Cuenta Básica</h4>
                                <h5 class="card-title text-muted text-uppercase text-center">Para conocer Statista</h5>
                                <hr>
                                <p class="text-muted ">Acceso únicamente a estadísticas básicas.</p>
                                <input style="background-color:transparent;color:#33AEFF;" type="submit" value="Regístrese gratis" class="button--reset">
                            </div>
                        </div>
                    </div>
                    <!-- Plus-->
                    <div class="col-lg-5" style=" z-index:1;">
                        <div class="card mb-5">
                            <div class="card-body">
                                <h4 class="text-center">Cuenta Single</h4>
                                <h5 class="card-title text-muted text-uppercase text-center">Ideal para acceder a todas las estadísticas</h5>
                                <hr>
                                <ul class="fa-ul">
                                    <li><span class="fa-li"><i class="fas fa-check"></i></span><b>Unlimited Users</b> a un millón de estadísticas</li>
                                    <li><span class="fa-li"><i class="fas fa-check"></i></span><b> Descarga</b> en formatos XLS, PDF y PNG</li>
                                    <li><span class="fa-li"><i class="fas fa-check"></i></span>Referencia <b>a las fuentes</b></li>
                                </ul>
                                <hr>
                                <div class="row">
                                    <div class="col-md-6">
                                        <h6 class="card-price text-center">$49<span class="period">/month</span></h6>
                                    </div>
                                    <div class="col-md-6">
                                        <input type="submit" value="Adquirirla ahora" class="button button--medium button--green whiteSpace--nowrap">
                                    </div>
                                </div>


                            </div>
                        </div>
                    </div>
                    <!-- Pro -->
                    <div class="col-lg-3 mt-md-4">
                        <div class="card">
                            <div class="card-body ">
                                <h6 class=" text-center">Cuenta Corporativa</h6>
                                <h5 class="card-title text-muted text-uppercase text-center">Acceso completo</h5>
                                <hr>
                                <p class="text-muted">La mejor solución para empresas</p>
                                <input style="background-color:transparent;color:#33AEFF;" type="submit" value="Regístrese gratis" class="button--reset">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>

        <!--nav verticar-->
        <section style="border: 2px double rgba(128, 128, 128, 0.20);">
            <h4 class="text-center mb-md-5 mt-md-5"><strong>Estadísticas sobre </strong>"El consumo de prensa en España"</h4>
            <div class="row" style="margin-bottom:20px;">
                <div class="col-10 col-md-3">
                    <div class="list-group" id="list-tab" role="tablist">
                        @foreach (var item in ViewBag.listaAsociado)
                         {
                            <a class="list-group-item list-group-item-action" id="list-@item.nombre-list" data-toggle="list" href="#list-@item.nombre" role="tab" aria-controls="@item.nombre" style="color:#000;">@item.nombre</a>
                         }
                        @*
                             <a class="list-group-item list-group-item-action" id="list-settings-list" data-toggle="list" href="#list-settings" role="tab" aria-controls="settings" style="color:#000;">Empresas</a>
                        *@
                    </div>

                </div>
                <div class="col-md-8 ">
                    <h6 style="color:#000;margin-top:10px;">Las estadísticas más importantes</h6>
                    <div class="tab-content" id="nav-tabContent">
                        @foreach (var item in ViewBag.listaAsociado)
                        {
                            <div class="tab-pane fade" id="list-@item.nombre" role="tabpanel" aria-labelledby="list-@item.nombre-list">@item.descripcion</div>
                        }
                        <div class="tab-pane fade show active" id="list-home" role="tabpanel" aria-labelledby="list-home-list">Las estadísticas más importantes</div>
                        @*
                        <div class="tab-pane fade" id="list-settings" role="tabpanel" aria-labelledby="list-settings-list">4 Empresas</div>*@
                    </div>
                </div>
            </div>
        </section>

        <section style="border: 2px double rgba(128, 128, 128, 0.20); box-sizing:border-box;  width: 100%;">
            <!--buscador search bar-->
            <div class="row justify-content-center mb-5 mt-md-5 text-center" style="background:#fff; ">
                <div class="col-12 col-md-10 col-lg-8">
                    <h4 style="color:#424242;margin-bottom:30px;">Descubra Data Intelligence</h4>
                    <form class="card card-sm">
                        <div class="card-body row no-gutters align-items-center">
                            <div class="col-auto">
                                <i class="h4 text-body"></i>
                            </div>
                            <!--end of col-->
                            <div class="col">
                                <input class="form-control form-control-lg form-control-borderless" id="myInput" type="text" name="myCountry" placeholder="Encuentre estadísticas, y estudios">

                            </div>
                            <!--end of col-->
                            <div class="col-auto">
                                <button class="btn btn-lg btn-success mb-md-4" type="submit">Search</button>
                            </div>
                            <!--end of col-->
                        </div>
                    </form>
                </div>
                <!--end of col-->
            </div>

        </section>
        <!--Seccion con scrollbar-->
        <section style="margin-bottom:100px; background:#F7F9FA">
            <br />
            <br />
            <h4 class="text-center mb-md-4"><strong>Otros contenidos:</strong> También le podría interesar</h4>
            <strong style="margin-left: 80px;">Estadisticas</strong>
            <div class="row container ml-md-5" style="list-style:none;">

                <div class="col-md-7" style="width:600px; height:350px; overflow-x: hidden!important; overflow-y: scroll; margin-bottom:50px; background:#fff;">

                    <ul class="list-group mb-md-7" style="list-style:none">

                        @foreach(var item in ViewBag.listaOtrosContenidos) {
                        <li class="mt-md-3"><span><img class="mr-md-2 mb-md-2" src="~/Content/iconos_PopUp/barra-grafica.png" alt="Alternate Text" /></span>@item.nombre</li>
                        }
                        @*
                        <li><span><img class="mr-md-2 mb-md-2" src="~/Content/iconos_PopUp/barra-grafica.png" alt="Alternate Text" /></span>A simple primary list group item</li>
                        
                        <li><span><img class="mr-md-2 mb-md-2" src="~/Content/iconos_PopUp/barra-grafica.png" alt="Alternate Text" /></span>A simple secondary list group item</li>
                        
                        @for (int i = 0; i < 10; i++)
                        {
                        <li><span><img class="mr-md-2 mb-md-2" src="~/Content/iconos_PopUp/barra-grafica.png" alt="Alternate Text" /></span>@i</li>
                        }
                         *@
                    </ul>
                </div>
                <div class="col-md-5">

                    <div class="row">

                        <div class="col-md-2 mb-md-4">
                            <img src="~/Content/img/Diseño sin título - copia.png" />
                        </div>
                        <div class="col-md-9 ml-md-3">
                            <p>
                                @*
        @ViewBag.Resultado.TITULO
                                *@
                            </p>
                        </div>
                    </div>
                </div>
            </div>

        </section>

        <!--Seccion de call to action-->
        <section class="call-to-action">
            <div class="container">
                <div class="row">
                    <div class="col-md-8 col-sm-8 text-right m-text-center">
                        <h5>¿Desea conocer más sobre nuestras soluciones profesionales?</h5>
                        <p id="last">Le enviaremos encantados información detallada sobre nuestra Cuenta Corporativa.</p>
                    </div>
                    <div class="col-md-4 col-sm-4 m-text-center">
                        <a href="https://dataintelligence.store/pages/contacto-1" target="_blank" class="btn btn-white">Solicitar Informacion</a>
                    </div>
                </div>
            </div>
        </section>
    </div>

    <footer class="row">
        <div class="col">
            <h4 style="padding-left:50px;">Footer</h4>
        </div>

    </footer>
    <!--script de buscador-->
    <script src="~/Scripts/ScriptBuscador.js"></script>
    <!--Script de ocultar texto-->
    <script src="~/Scripts/OcultarBoton.js"></script>
    <!--Jquery, popper y Bootstrap-->
    <script src="~/js/jquery-3.3.1.slim.min.js"></script>
