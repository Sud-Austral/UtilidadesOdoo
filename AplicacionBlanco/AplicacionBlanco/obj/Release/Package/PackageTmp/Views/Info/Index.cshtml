                                        <div class="panel panel-default">
                                            <div class="card">
                                                @* <a href="~/info/index?id=@ViewBag.Elemento.id" style="color:#000;">*@
                                                <a href="~/info/index?id=@item.id" style="color:#000;">
                                                    <img class="img-fluid " alt="100%x280" src="~/Content/iconos_PopUp/gracircular (1).png">

                                                    <div class="card-body">
                                                        <h4 class="card-title text-center">@item.titulo</h4>
                                                        <p class="card-text text-center"> @item.descripcion</p>

                                                    </div>
                                                </a>
                                            </div>
                                        </div>
                                    </div>
                                 }

                            </div>

                        </div>
                    </div>
                </div>
            </div>
