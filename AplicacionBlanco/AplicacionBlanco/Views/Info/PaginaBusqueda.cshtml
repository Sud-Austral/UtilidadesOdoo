                                        <script>
                                            setTimeout(function () {
                                                $('.results > td').hide();
                                            }, 1);

                                            $(".tags input").on("click", function () {
                                                if ($(this).attr("id") == "todos") {
                                                    $("#arts, #computers, #health, #video-games").prop('checked', false);
                                                    filtrar(true);
                                                } else {
                                                    $("#todos").prop('checked', false);
                                                    filtrar(false);
                                                }
                                            });

                                            function filtrar(e) {
                                                if (e) {
                                                    $(".results td").each(function () {
                                                        $(this).show();
                                                    });
                                                } else {
                                                    $('.results td').hide();
                                                    $('.tags').find('input:checked').each(function () {
                                                        $('.results td.' + $(this).attr('id')).show();
                                                    });
                                                }
                                            }
                                        </script>
