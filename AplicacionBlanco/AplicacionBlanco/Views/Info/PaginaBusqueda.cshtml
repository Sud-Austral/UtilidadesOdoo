                                                    <tr>
                                                        <td class="arts">@item.nombre</td>
                                                        <td class="computers" >@item.TEMPORALIDAD.nombre</td>
                                                        <td class="health">@item.TERRITORIO.nombre</td>
                                                        <td class="video-games" >@item.DETALLE.nombre</td>
