@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Mi aplicación ASP.NET</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>

    <div class="row justify-content-center">
        <div class="col-5 offset-1">
            <a class="twitter-timeline"
               data-height="600"
               data-width="550"
               href="https://twitter.com/data_int?ref_src=twsrc%5Etfw">Tweets by data_int</a>
            <script async src="https://platform.twitter.com/widgets.js" charset="utf-8"></script>
        </div>
        <div class="col-5 offset-1">
            <div id="fb-root"></div>
            <script async defer crossorigin="anonymous" src="https://connect.facebook.net/es_LA/sdk.js#xfbml=1&version=v8.0&appId=39190517462&autoLogAppEvents=1" nonce="qUESwvCG"></script>
            <div class="fb-page"
                 style="width:auto !important"
                 data-tabs="timeline"
                 data-width="700"
                 data-height="600"
                 data-href="https://www.facebook.com/dataintelligencecl"
                 data-hide-cover="false"
                 data-show-facepile="true"></div>
        </div>

    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
</body>
</html>

